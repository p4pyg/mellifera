{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "additionnals-vendors-packages-docs":[
        "http://packalyst.com/packages/package/ludo237/barcode",
        "http://packalyst.com/packages/package/thujohn/pdf"
    ],
    "require": {
        "laravel/framework": "4.2.*",
        "vinelab/http": "1.2.0",
        "barryvdh/laravel-debugbar":"~1.8"
    },
    "require-dev": {
        "ludo237/barcode": "dev-master",
        "thujohn/pdf": "dev-master"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "app/Mellifera/Auth/MelliferaAuthProvider.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
